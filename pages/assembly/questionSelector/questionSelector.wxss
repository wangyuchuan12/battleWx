/* questionSelector.wxss */
.questionSelector{
  background:RGBA(44,51,59,1);
  width:70%;
  height:50vh;
  left:50%;
  position: absolute;
  top:30vh;
  margin-left:-39%;

  border:15px solid RGBA(33,39,47,1);

  border-radius:10px;

  
}

.questionSelectorHeader{
  width:100%;

  text-align: center;
}

.questionSelectorHeaderList{
  width:100%;
  display:block;

  text-align: left;
}

.questionSelectorHeaderItem{
  width:50px;
  height:60px;
  display: inline-block;
  position:relative;

  border:1px solid black;
}


.questionSelectorHeaderItemImg{
  width:100%;
  height:60px;
}

.questionSelectorHeaderItemTest{
  font-size:10px;
  width:100%;
  text-align:center;
  background:RGBA(33,39,47,0.5);
  position:absolute;
  top:45px;
  color:white;
}

.questionSelectorContentList{
  width:100%;
  height:37vh;
}

.questionSelectorContent{
 
  display:block;

  padding-top:20px;
  
}


.questionSelectorContentItem{
  width:31%;
  height:100px;
  display: inline-block;
  position:relative;

  border:1px solid black;

}

.questionSelectorContentItemTest{
  font-size:15px;
  width:100%;
  height:20px;
  text-align:center;
  background:RGBA(33,39,47,0.5);
  position:absolute;
  top:78px;
  color:white;
  overflow:hidden;
}

.questionSelectorContentItemImg{
  width:100%;
  height:90px;
}